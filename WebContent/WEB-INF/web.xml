<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Orbits_v1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>/websocket/chat.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>jws_config</param-name>
    <param-value>${WEB_APP_HOME}WEB-INF/jWebSocket.xml</param-value>
  </context-param>
  <listener>
    <description>Context listener used to start and stop the jWebSocket application server.</description>
    <listener-class>org.jwebsocket.tomcat.ContextListener</listener-class>
  </listener>
  <listener>
    <description>Session management listener</description>
    <listener-class>org.jwebsocket.tomcat.listener.SessionListener</listener-class>
  </listener>
  <servlet-mapping>
    <servlet-name>jWebSocket</servlet-name>
    <url-pattern>/jWebSocket</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>jWebSocket</servlet-name>
    <servlet-class>org.jwebsocket.tomcat.TomcatServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>MyServlet</display-name>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>com.node.create.MyServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>DisplayNodesServlet</display-name>
    <servlet-name>DisplayNodesServlet</servlet-name>
    <servlet-class>com.node.display.DisplayNodesServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>SessionInvalidate</display-name>
    <servlet-name>SessionInvalidate</servlet-name>
    <servlet-class>com.node.logout.SessionInvalidate</servlet-class>
  </servlet>
  <servlet>
    <display-name>UpvoteServlet</display-name>
    <servlet-name>UpvoteServlet</servlet-name>
    <servlet-class>com.node.upvote.UpvoteServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>DownvoteServlet</display-name>
    <servlet-name>DownvoteServlet</servlet-name>
    <servlet-class>com.node.downvote.DownvoteServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>EditServlet</display-name>
    <servlet-name>EditServlet</servlet-name>
    <servlet-class>com.node.edit.EditServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>CommentServlet</display-name>
    <servlet-name>CommentServlet</servlet-name>
    <servlet-class>com.node.edit.CommentServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>CreateNewTree</display-name>
    <servlet-name>CreateNewTree</servlet-name>
    <servlet-class>com.node.create.CreateNewTree</servlet-class>
  </servlet>
  <servlet>
    <display-name>AuthAndDisplayProjects</display-name>
    <servlet-name>AuthAndDisplayProjects</servlet-name>
    <servlet-class>com.user.project.AuthAndDisplayProjects</servlet-class>
  </servlet>
  <servlet>
    <display-name>ImportanceVoteServlet</display-name>
    <servlet-name>ImportanceVoteServlet</servlet-name>
    <servlet-class>com.node.priority.ImportanceVoteServlet</servlet-class>
  </servlet>
    <servlet>
    <display-name>RegistrationServlet</display-name>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.user.registration.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>cwServlet</servlet-name>
    <servlet-class>websocket.chat.ChatWebSocketServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cwServlet</servlet-name>
    <url-pattern>/updatedata</url-pattern>
  </servlet-mapping>
  <security-role>
    <role-name>role1</role-name>
  </security-role>
  <security-role>
    <role-name>tomcat</role-name>
  </security-role>
</web-app>